{
  "name": "vs-org",
  "displayName": "Vs-Org",
  "description": "Quickly create todo lists, take notes, plan projects and organize your thoughts.",
  "version": "0.0.2",
  "repository": "https://www.github.com/robaboyd/vs-org",
  "publisher": "BobbyBoyd",
  "license": "MIT",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createVsoFile",
    "onCommand:extension.getTags",
    "onCommand:extension.getTitles",
    "onCommand:extension.setFolderPath",
    "onLanguage:vso"
  ],
  "keywords": [
    "vsorg",
    "todo",
    "todos",
    "org-mode",
    "list",
    "vs-org",
    "organizer",
    "scheduler"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "VS-Org Config",
      "properties": {
        "vsorg.folderPath": {
          "type": "string",
          "default": "",
          "description": "Main folder"
        }
      }
    },
    "commands": [
      {
        "command": "extension.setFolderPath",
        "title": "VS-Org: Change VS-Org Directory"
      },
      {
        "command": "extension.createVsoFile",
        "title": "VS-Org: Create New .vsorg file"
      },
      {
        "command": "extension.getTags",
        "title": "VS-Org: Open By Tag"
      },
      {
        "command": "extension.getTitles",
        "title": "VS-Org: Open By Title "
      }
    ],
    "keybindings": [
      {
        "command": "extension.toggleStatusRight",
        "key": "shift+right",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.toggleStatusLeft",
        "key": "shift+left",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.moveBlockUp",
        "key": "shift+alt+up",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.moveBlockDown",
        "key": "shift+alt+down",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.increment",
        "key": "alt+right",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.decrement",
        "key": "alt+left",
        "when": "editorTextFocus"
      }
    ],
    "languages": [
      {
        "id": "vso",
        "aliases": [
          "VSOrg",
          "vsorg",
          "vso"
        ],
        "extensions": [
          ".vsorg",
          ".vso"
        ]
      }
    ],
    "grammars": [
      {
        "language": "vso",
        "scopeName": "source.vso",
        "path": "vso.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "vso",
        "path": "./snippets/vso.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "fs-extra": "^7.0.0",
    "gray-matter": "^4.0.1",
    "klaw": "^3.0.0"
  }
}
