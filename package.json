{
  "name": "vs-org",
  "displayName": "VS-Org",
  "description": "Quickly create todo lists, take notes, plan projects and organize your thoughts.",
  "version": "0.1.4",
  "repository": "https://www.github.com/robaboyd/vs-org",
  "icon": "potentialLogo.png",
  "publisher": "BobbyBoyd",
  "license": "MIT",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "keywords": [
    "todo",
    "org-mode",
    "list",
    "organizer",
    "scheduler"
  ],
  "main": "./out/extension",
  "contributes": {
    "configurationDefaults": {
      "[vso]": {
        "editor.formatOnType": true,
        "files.insertFinalNewline": true
      }
    },
    "configuration": {
      "type": "object",
      "title": "VS-Org Config",
      "properties": {
        "vsorg.folderPath": {
          "type": "string",
          "default": "",
          "description": "Main folder"
        },
        "vsorg.dateFormat": {
          "type": "string",
          "default": [
            "MM-DD-YYYY"
          ],
          "enum": [
            "MM-DD-YYYY",
            "DD-MM-YYYY"
          ],
          "minItems": 1,
          "maxItems": 3,
          "uniqueItems": true,
          "description": "The scheduled date format."
        }
      }
    },
    "commands": [{
        "command": "extension.setFolderPath",
        "title": "VS-Org: Change VS-Org Directory"
      },
      {
        "command": "extension.createVsoFile",
        "title": "VS-Org: Create New .vsorg file"
      },
      {
        "command": "extension.getTags",
        "title": "VS-Org: Open By Tag"
      },
      {
        "command": "extension.getTitles",
        "title": "VS-Org: Open By Title "
      },
      {
        "command": "extension.viewAgenda",
        "title": "VS-Org: Agenda View"
      }
    ],
    "keybindings": [{
        "command": "extension.toggleStatusRight",
        "key": "shift+right",
        "when": "editorLangId == 'vso' && editorTextFocus"
      },
      {
        "command": "extension.toggleStatusLeft",
        "key": "shift+left",
        "when": "editorLangId == 'vso' && editorTextFocus"
      },
      {
        "command": "extension.moveBlockUp",
        "key": "shift+alt+up",
        "when": "editorLangId == 'vso' && editorTextFocus"
      },
      {
        "command": "extension.moveBlockDown",
        "key": "shift+alt+down",
        "when": "editorLangId == 'vso' && editorTextFocus"
      },
      {
        "command": "extension.increment",
        "key": "alt+right",
        "when": "editorLangId == 'vso' && editorTextFocus"
      },
      {
        "command": "extension.decrement",
        "key": "alt+left",
        "when": "editorLangId == 'vso' && editorTextFocus"
      },
      {
        "command": "editor.fold",
        "key": "ctrl+shift+[",
        "when": "editorLangId == 'vso' && editorTextFocus"
      },
      {
        "command": "editor.unfold",
        "key": "ctrl+shift+]",
        "when": "editorLangId == 'vso' && editorTextFocus"
      },
      {
        "command": "extension.scheduling",
        "key": "ctrl+alt+s",
        "when": "editorLangId == 'vso' && editorTextFocus"
      }
    ],
    "languages": [{
      "id": "vso",
      "aliases": [
        "VSOrg",
        "vsorg",
        "vso"
      ],
      "extensions": [
        ".vsorg",
        ".vso"
      ]
    }],
    "grammars": [{
      "language": "vso",
      "scopeName": "source.vso",
      "path": "vso.tmLanguage.json"
    }],
    "snippets": [{
      "language": "vso",
      "path": "./snippets/vso.json"
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "typescript": "^5.5.4",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "fs-extra": "^7.0.0",
    "gray-matter": "^4.0.1",
    "klaw": "^3.0.0",
    "moment": "^2.22.2",
    "opn": "^5.4.0"
  }
}