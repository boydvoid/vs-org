{
  "name": "vs-org",
  "displayName": "Vs-Org",
  "description": "Org-Mode lite for vscode",
  "version": "0.0.1",
  "publisher": "Bobby",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vso"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.toggleStatusRight",
        "title": "VS-Org: Toggle TODO or DONE"
      },
      {
        "command": "extension.toggleStatus",
        "title": "VS-Org: Toggle TODO or DONE"
      },
      {
        "command": "extension.moveBlockUp",
        "title": "VS-Org: Move Block Up"
      },
      {
        "command": "extension.createVsoFile",
        "title": "VS-Org: Create New .vsorg file"
      }
    ],
    "keybindings": [
      {
        "command": "extension.toggleStatusRight",
        "key": "shift+right",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.toggleStatusLeft",
        "key": "shift+left",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.moveBlockUp",
        "key": "shift+alt+up",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.moveBlockDown",
        "key": "shift+alt+down",
        "when": "editorTextFocus"
      }
    ],
    "languages": [
      {
        "id": "vso",
        "aliases": [
          "VSOrg",
          "vsorg",
          "vso"
        ],
        "extensions": [
          ".vsorg",
          ".vso"
        ]
      }
    ],
    "grammars": [
      {
        "language": "vso",
        "scopeName": "source.vso",
        "path": "vso.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "vso",
        "path": "./snippets/vso.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
